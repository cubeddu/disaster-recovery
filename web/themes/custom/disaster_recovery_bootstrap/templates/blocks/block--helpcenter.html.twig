{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'clearfix',
    'row',
    'background-height'
  ]
%}
<section{{ attributes.addClass(classes) }} style="background-image:url({{ content.field_background.0}} ) ">
  {% block content %}
    <div class="container">
      <div class="panel panel-default col-md-6 col-md-offset-3 margin-6">
        <div class="panel-body text-center view-row-padding padding-bottom-20px">
        <h1{{ title_attributes.addClass(['lead', 'font-40']) }}>How can we help you?</h1>
          {{ drupal_block('exposedformknowledge_base_help_center_page_page_1', FALSE) }}
        </div>
      </div>
    </div>

  {% endblock %}
</section>
